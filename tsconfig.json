{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "resolveJsonModule": true,
    "allowJs": true,
    "sourceMap": true,
    "noImplicitThis": true,
    "removeComments": false,
    "experimentalDecorators": true,
    "jsx": "react",
    "outDir": "dist",
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "static"
  ],
  "awesomeTypescriptLoaderOptions": {
    "silent": true
  }
}